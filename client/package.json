{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development next dev",
    "build": "cross-env NODE_ENV=production next build",
    "start": "next start",
    "server": "cd ../server && yarn start:dev",
    "client": "yarn dev",
    "start:dev": "concurrently \"yarn server\" \"yarn client\"",
    "lint": "next lint",
    "lint:fix": "eslint --fix \"**/*.+(js|jsx|ts|tsx)\"",
    "prettier": "prettier --check \"**/*.+(js|jsx|ts|tsx|json)\"",
    "prettier:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx|json)\"",
    "prepare": "cd .. && husky install client/.husky",
    "stylelint": "stylelint \"**/*.scss\" --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@hookform/resolvers": "^2.9.10",
    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.10.5",
    "@mui/styled-engine-sc": "^5.10.3",
    "@reduxjs/toolkit": "^1.9.0",
    "axios": "^0.27.2",
    "dotenv": "^16.0.3",
    "next": "12.3.0",
    "next-redux-wrapper": "^8.0.0",
    "react": "18.2.0",
    "react-admin": "^4.5.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.39.1",
    "react-redux": "^8.0.5",
    "sass": "^1.55.0",
    "styled-components": "^5.3.5",
    "yarn": "^1.22.19",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/node": "18.7.18",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "concurrently": "^7.5.0",
    "cross-env": "^7.0.3",
    "eslint": "8.23.1",
    "eslint-config-next": "12.3.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "stylelint": "^14.14.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-order-config-standard": "^0.1.3",
    "typescript": "4.8.3"
  }
}
